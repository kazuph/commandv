{
  "name": "commandv",
  "version": "1.0.0",
  "description": "An instant previewer for Claude Artifacts. Just paste your component code and see it rendered immediately.",
  "type": "module",
  "license": "MIT",
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/kazuph/commandv.git"
  },
  "keywords": [
    "react",
    "component",
    "preview",
    "paste",
    "visualizer",
    "cloudflare",
    "workers",
    "hono"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode client && vite build",
    "preview": "wrangler dev server-build/index.js",
    "deploy": "$npm_execpath run build && wrangler deploy server-build/index.js"
  },
  "dependencies": {
    "@babel/standalone": "^7.26.10",
    "@monaco-editor/react": "^4.7.0",
    "hono": "^4.6.9",
    "lodash": "^4.17.21",
    "papaparse": "^5.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "recharts": "^2.15.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241106.0",
    "@hono/vite-build": "^1.1.0",
    "@hono/vite-dev-server": "^0.16.0",
    "@tailwindcss/postcss": "^4.0.14",
    "@tailwindcss/vite": "^4.0.14",
    "@types/lodash": "^4.17.16",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^18.2.60",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.14",
    "vite": "^5.4.10",
    "wrangler": "^3.86.0"
  }
}
